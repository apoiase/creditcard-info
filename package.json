{
  "name": "creditcard-warder",
  "version": "0.2.7",
  "description": "Rules from credit card from Brazil",
  "main": "index.js",
  "scripts": {
    "test": "mocha --ui tdd",
    "test-browser": "node_modules/.bin/testling",
    "build": "node_modules/.bin/browserify  index.js --standalone CreditcardWarder -o dist/creditcard-warder.js && npm run build-min",
    "build-min": "uglifyjs dist/creditcard-warder.js -o dist/creditcard-warder.min.js --compress",
    "watch": "node_modules/.bin/watchify index.js -o dist/creditcard-warder.js --debug --verbose"
  },
  "contributors": [
    {
      "name": "Ariel Pchara",
      "email": "ariel@pchara.com",
      "url": "http://ariel.pchara.com"
    }, {
      "name": "David Basilio",
      "email": "dbasilioesp@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:apoiase/creditcard-warder.git"
  },
  "keywords": [
    "creditcard",
    "validations",
    "brazilian",
    "brands"
  ],
  "license": "MIT",
  "dependencies": {
    "lodash": "^3.10.1",
    "luhn": "^2.1.0"
  },
  "devDependencies": {
    "browserify": "^14.1.0",
    "expect.js": "^0.3.1",
    "mocha": "^3.2.0",
    "testling": "^1.7.1",
    "uglify-js": "^2.8.1",
    "watchify": "^3.9.0"
  },
  "testling": {
    "harness": "mocha",
    "files": "test/*.js",
    "browsers": [
      "chrome/latest",
      "firefox/latest"
    ]
  }
}
